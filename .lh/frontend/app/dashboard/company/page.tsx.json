            "name": "Commit-0",
            "content": "\"use client\"\r\n\r\nimport { Card, CardContent, CardHeader, CardTitle } from \"@/components/ui/card\"\r\nimport { Button } from \"@/components/ui/button\"\r\nimport { Badge } from \"@/components/ui/badge\"\r\nimport { Progress } from \"@/components/ui/progress\"\r\nimport { useRandomStars } from \"@/hooks/use-random-stars\"\r\nimport { Briefcase, Users, Star, TrendingUp } from \"lucide-react\"\r\n\r\nconst projects = [\r\n  {\r\n    name: \"Proyecto Alpha\",\r\n    description: \"Desarrollo de UI/UX para aplicación móvil\",\r\n    progress: 75,\r\n    team: 4,\r\n    status: \"En progreso\",\r\n  },\r\n  {\r\n    name: \"Proyecto Beta\",\r\n    description: \"Diseño de sistema de diseño\",\r\n    progress: 45,\r\n    team: 3,\r\n    status: \"En revisión\",\r\n  },\r\n  {\r\n    name: \"Proyecto Gamma\",\r\n    description: \"Prototipado de nueva plataforma\",\r\n    progress: 90,\r\n    team: 5,\r\n    status: \"Casi completo\",\r\n  },\r\n]\r\n\r\nexport default function CompanyDashboard() {\r\n  const stars = useRandomStars(50)\r\n\r\n  return (\r\n    <div className=\"relative\">\r\n      {/* Stars Background */}\r\n      <div className=\"fixed inset-0 -z-10 overflow-hidden\">\r\n        {stars.map((star) => (\r\n          <div\r\n            key={star.id}\r\n            className={`absolute inline-flex animate-pulse ${star.color}`}\r\n            style={{\r\n              left: `${star.x}%`,\r\n              top: `${star.y}%`,\r\n              width: `${star.size}px`,\r\n              height: `${star.size}px`,\r\n              opacity: star.opacity,\r\n              animation: `pulse ${star.duration}s infinite`,\r\n            }}\r\n          >\r\n            <span className=\"absolute inline-flex h-full w-full animate-ping rounded-full bg-primary opacity-75\" />\r\n            <span className=\"relative inline-flex h-full w-full rounded-full bg-primary\" />\r\n          </div>\r\n        ))}\r\n      </div>\r\n\r\n      <div className=\"space-y-6 p-6\">\r\n        <Card className=\"border-primary/20 bg-black/50 backdrop-blur-sm\">\r\n          <CardHeader>\r\n            <CardTitle className=\"text-2xl font-bold text-white\">Panel Empresarial</CardTitle>\r\n          </CardHeader>\r\n          <CardContent className=\"grid gap-6\">\r\n            {projects.map((project, index) => (\r\n              <div key={index} className=\"rounded-lg border border-primary/20 bg-primary/5 p-6\">\r\n                <div className=\"mb-4 flex items-center justify-between\">\r\n                  <div>\r\n                    <h3 className=\"text-lg font-medium text-white\">{project.name}</h3>\r\n                    <p className=\"text-sm text-gray-400\">{project.description}</p>\r\n                  </div>\r\n                  <Badge variant=\"outline\" className=\"border-primary/20 bg-primary/10 text-primary\">\r\n                    {project.status}\r\n                  </Badge>\r\n                </div>\r\n                <div className=\"mb-4 space-y-2\">\r\n                  <div className=\"flex justify-between text-sm\">\r\n                    <span className=\"text-gray-400\">Progreso</span>\r\n                    <span className=\"text-white\">{project.progress}%</span>\r\n                  </div>\r\n                  <Progress value={project.progress} className=\"h-2\" />\r\n                </div>\r\n                <div className=\"flex items-center justify-between\">\r\n                  <div className=\"flex items-center gap-2 text-sm text-gray-400\">\r\n                    <Users className=\"h-4 w-4\" />\r\n                    <span>{project.team} miembros</span>\r\n                  </div>\r\n                  <Button variant=\"ghost\" className=\"text-primary hover:text-primary/80\">\r\n                    Ver detalles\r\n                  </Button>\r\n                </div>\r\n              </div>\r\n            ))}\r\n          </CardContent>\r\n        </Card>\r\n\r\n        <div className=\"grid gap-6 md:grid-cols-3\">\r\n          <Card className=\"border-primary/20 bg-black/50 backdrop-blur-sm\">\r\n            <CardContent className=\"flex items-center gap-4 p-6\">\r\n              <div className=\"rounded-full bg-primary/20 p-3\">\r\n                <Briefcase className=\"h-6 w-6 text-primary\" />\r\n              </div>\r\n              <div>\r\n                <p className=\"text-sm text-gray-400\">Proyectos Activos</p>\r\n                <p className=\"text-2xl font-bold text-white\">12</p>\r\n              </div>\r\n            </CardContent>\r\n          </Card>\r\n\r\n          <Card className=\"border-primary/20 bg-black/50 backdrop-blur-sm\">\r\n            <CardContent className=\"flex items-center gap-4 p-6\">\r\n              <div className=\"rounded-full bg-primary/20 p-3\">\r\n                <Star className=\"h-6 w-6 text-primary\" />\r\n              </div>\r\n              <div>\r\n                <p className=\"text-sm text-gray-400\">Talentos Destacados</p>\r\n                <p className=\"text-2xl font-bold text-white\">24</p>\r\n              </div>\r\n            </CardContent>\r\n          </Card>\r\n\r\n          <Card className=\"border-primary/20 bg-black/50 backdrop-blur-sm\">\r\n            <CardContent className=\"flex items-center gap-4 p-6\">\r\n              <div className=\"rounded-full bg-primary/20 p-3\">\r\n                <TrendingUp className=\"h-6 w-6 text-primary\" />\r\n              </div>\r\n              <div>\r\n                <p className=\"text-sm text-gray-400\">Tasa de Éxito</p>\r\n                <p className=\"text-2xl font-bold text-white\">92%</p>\r\n              </div>\r\n            </CardContent>\r\n          </Card>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\n"
        }
    ]
